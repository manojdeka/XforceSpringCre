<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="car" class="com.dev.xforcespringcore.example.Car">
		<property name="model" value="x1"/>
		<property name="brand" value="audi" />
		<property name="type" value="petrol"/>
		<property name="engine" ref="honda"/>
	</bean>
	
	<bean id="audi" class="com.dev.xforcespringcore.example.Audi">
	</bean>
	
	<bean id="honda" class="com.dev.xforcespringcore.example.Honda"/> 
</beans>